{
	"compileOnSave": false,
	"compilerOptions": {
		"jsx": "preserve",
		"allowJs": true,
		"checkJs": true,
		"noEmit": true,
		"module": "ES2020",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"downlevelIteration": true,
		"target": "ESNext"
	},
	"typeAcquisition": {
		"enable": true
	}
}
