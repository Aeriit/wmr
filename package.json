{
  "name": "wmr",
  "version": "0.1.0",
  "bin": "wmr.cjs",
  "type": "module",
  "scripts": {
    "demo": "cd demo && node --experimental-modules ../src/cli.js 2>&1 | grep -v ExperimentalWarning",
    "dev": "PROFILE=true nodemon -w src --exec \"npm run -s demo\"",
    "build": "rollup -c",
    "test": "eslint && jest --runInBand"
  },
  "author": "Jason Miller <jason@developit.ca> (http://jasonformat.com)",
  "files": [
    "src",
    "wmr.js"
  ],
  "eslintConfig": {
    "extends": [
      "developit",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-console": "off",
      "prefer-rest-params": "off",
      "no-prototype-builtins": "off",
      "function-call-argument-newline": [
        "error",
        "consistent"
      ],
      "react/jsx-no-bind": "off"
    },
    "globals": {
      "jestPuppeteer": "readonly",
      "browser": "readonly",
      "page": "readonly"
    }
  },
  "eslintIgnore": [
    "wmr.cjs"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "useTabs": true,
    "printWidth": 120,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "{src,test}/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "preset": "jest-puppeteer"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@types/jest": "^26.0.0",
    "acorn-jsx": "^5.2.0",
    "acorn-jsx-walk": "^2.0.0",
    "acorn-walk": "^7.1.1",
    "async-disk-cache": "^2.0.0",
    "babel-jest": "^26.0.1",
    "chokidar": "^3.4.0",
    "devcert": "^1.1.0",
    "eslint": "^7.0.0",
    "eslint-config-developit": "^1.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "gunzip-maybe": "^1.4.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-puppeteer": "^4.4.0",
    "lint-staged": "^10.2.4",
    "magic-string": "^0.25.7",
    "ncp": "^2.0.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.4",
    "npm-merge-driver-install": "^1.1.1",
    "polka": "^0.5.2",
    "prettier": "^2.0.5",
    "puppeteer": "^3.3.0",
    "rollup": "^2.11.2",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "sade": "^1.7.3",
    "semver": "^7.3.2",
    "sirv": "^1.0.0",
    "tar-stream": "^2.1.2",
    "terser": "^4.7.0",
    "tiny-glob": "^0.2.6",
    "tmp-promise": "^3.0.2",
    "ws": "^7.3.0"
  },
  "engines": {
    "node": ">=12.16"
  }
}
