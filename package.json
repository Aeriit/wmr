{
  "name": "wmr",
  "version": "0.1.0",
  "bin": "wmr.cjs",
  "type": "module",
  "scripts": {
    "demo": "cd demo && node --experimental-modules ../src/cli.js",
    "build": "rollup -c",
    "test": "eslint"
  },
  "author": "Jason Miller <jason@developit.ca> (http://jasonformat.com)",
  "files": [
    "src",
    "wmr.js"
  ],
  "eslintConfig": {
    "extends": [
      "developit",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-console": "off",
      "prefer-rest-params": "off",
      "no-prototype-builtins": "off",
      "function-call-argument-newline": [
        "error",
        "consistent"
      ]
    }
  },
  "eslintIgnore": [
    "wmr.cjs"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "useTabs": true,
    "printWidth": 120,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "{src,test}/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "acorn-jsx": "^5.2.0",
    "acorn-jsx-walk": "^2.0.0",
    "acorn-walk": "^7.1.1",
    "async-disk-cache": "^2.0.0",
    "compression": "^1.7.4",
    "eslint": "^7.0.0",
    "eslint-config-developit": "^1.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4",
    "magic-string": "^0.25.7",
    "node-fetch": "^2.6.0",
    "npm-merge-driver-install": "^1.1.1",
    "polka": "^0.5.2",
    "prettier": "^2.0.5",
    "rollup": "^2.10.5",
    "rollup-plugin-preserve-shebang": "^1.0.0",
    "sade": "^1.7.3",
    "sirv": "^0.4.2",
    "terser": "^4.7.0",
    "tiny-glob": "^0.2.6",
    "wss": "^3.3.4"
  }
}
